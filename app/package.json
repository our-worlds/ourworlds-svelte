{
  "name": "oneworld",
  "version": "0.0.1",
  "scripts": {
    "dev": "svelte-kit dev --host",
    "build": "svelte-kit build",
    "package": "svelte-kit package",
    "preview": "svelte-kit preview",
    "lint": "prettier --ignore-path .gitignore --check ./src/**/*.{js,svelte,ts} ./*.{ts,cjs,js} --plugin-search-dir=. && npm run eslint",
    "lint:fix": "prettier --check ./src/**/*.{js,svelte,ts} ./*.{ts,cjs,js} --plugin-search-dir=. && npm run eslint:fix",
    "format": "prettier --ignore-path .gitignore --write ./src/**/*.{js,svelte,ts} ./*.{ts,cjs,js} --plugin-search-dir=.",
    "eslint": "eslint ./src/**/*.{js,svelte,ts} --ignore-path .gitignore .eslintignore",
    "eslint:fix": "eslint ./src/**/*.{js,svelte,ts} --ignore-path .gitignore .eslintignore --fix",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^1.0.0-next.11",
    "@sveltejs/adapter-netlify": "^1.0.0-next.44",
    "@sveltejs/adapter-static": "^1.0.0-next.26",
    "@sveltejs/adapter-vercel": "^1.0.0-next.39",
    "@sveltejs/kit": "^1.0.0-next.247",
    "@tailwindcss/typography": "^0.5.2",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "autoprefixer": "^10.4.4",
    "cssnano": "^5.1.7",
    "cssnano-preset-advanced": "^5.3.3",
    "eslint": "^8.7.0",
    "eslint-config-airbnb-typescript": "^16.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-svelte3": "^3.4.0",
    "eslint-plugin-tsdoc": "^0.2.14",
    "postcss-cli": "^9.1.0",
    "postcss-load-config": "^3.1.4",
    "prettier": "^2.5.1",
    "prettier-plugin-svelte": "^2.6.0",
    "svelte": "^3.46.3",
    "svelte-check": "^2.4.6",
    "svelte-clipboard": "^1.0.0",
    "svelte-loader": "^3.1.2",
    "svelte-preprocess": "^4.10.5",
    "svelte-web3": "^3.3.2",
    "tailwindcss": "^3.0.23",
    "ts-node": "^10.7.0",
    "tslib": "^2.3.1",
    "typescript": "^4.6.3"
  },
  "type": "module",
  "dependencies": {
    "@sveltejs/pancake": "^0.0.18",
    "globe.gl": "^2.24.1",
    "leaflet": "^1.7.1",
    "lint-staged": "^12.3.7",
    "svelte-loading-spinners": "^0.1.7"
  }
}
